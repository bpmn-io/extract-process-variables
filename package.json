{
  "name": "@bpmn-io/extract-process-variables",
  "version": "2.0.0",
  "description": "A util for bpmn-js to extract Camunda BPM process variables from a BPMN 2.0 diagram.",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./zeebe": "./zeebe/index.js"
  },
  "source": "src/index.js",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint .",
    "pretest": "run-s build",
    "prepare": "run-s build",
    "dev": "mocha --reporter=spec --recursive --watch test",
    "test": "nyc --reporter=lcov mocha --reporter=spec --recursive test",
    "all": "run-s lint test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bpmn-io/extract-process-variables.git"
  },
  "author": "Niklas Kiefer <niklas.kiefer@camunda.com> (https://github.com/pinussilvestrus)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bpmn-io/extract-process-variables/issues"
  },
  "homepage": "https://github.com/bpmn-io/extract-process-variables#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "bpmn-moddle": "^10.0.0",
    "camunda-bpmn-moddle": "^7.0.1",
    "chai": "^6.2.2",
    "eslint": "^9.39.2",
    "eslint-plugin-bpmn-io": "^2.2.0",
    "mocha": "^11.7.5",
    "npm-run-all": "^4.1.5",
    "nyc": "^17.1.0",
    "rollup": "^4.57.1",
    "zeebe-bpmn-moddle": "^1.12.0"
  },
  "dependencies": {
    "min-dash": "^5.0.0"
  },
  "files": [
    "dist",
    "zeebe"
  ]
}
